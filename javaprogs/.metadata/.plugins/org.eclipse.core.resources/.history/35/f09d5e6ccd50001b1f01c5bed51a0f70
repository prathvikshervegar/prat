package see;

class First extends Thread{
	public void display(String msg) throws InterruptedException{
		 System.out.print("["+msg);
		 Thread.sleep(1000);
		 System.out.println("]");
	}
}

class Second extends Thread{
	First fobj;
	String msg;
	Second(First fp,String msg){
		fobj = fp;
		this.msg = msg;
		start();
	}
	public void run() {
		synchronized (fobj) {
			try {
				fobj.display(msg);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

}

public class P9 {

}
